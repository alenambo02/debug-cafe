    
<header>
    <div id="menu-image">
        <img id="logo" src="https://cdn.discordapp.com/attachments/1004083809429508196/1005365130462236763/cafe_image.png" alt="Cafe logo">
    </div> 
    
</header>

{{#each items as |item| }}
    <section id="container" class="row">
        <div id="menu-box" class="col-6">
            <div>
                <h3 id="item-title">{{ item.item_name }}</h3>
            </div>
            <div>   
                <p id="item-description">{{ item.item_description }}</p>
                <p id="item-price"> {{ item.price }}</p>
            </div>

            <div>
                <p id="item-title">{{ item.item_name }}</p>
            </div>
        </div> 
    </section>

{{/each}}

